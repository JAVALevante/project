<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.niit.entity.Cars" table="t_cars">
		<id name="h_id" column="h_id">
			<generator class="native"></generator>
		</id>
		<property name="h_name" column="h_name"></property>
		<property name="h_city" column="h_city"></property>
		<property name="h_distric" column="h_distric"></property>
		<property name="h_province" column="h_province"></property>
		<property name="h_x" column="h_x"></property>
		<property name="h_y" column="h_y"></property>
		<property name="h_area" column="h_area"></property>
		<property name="h_type" column="h_type"></property>
		<property name="h_RB" column="h_RB"></property>
		<property name="h_money" column="h_money"></property>
		<property name="h_date" column="h_date"></property>
		<property name="h_design" column="h_design"></property>
		<property name="h_info" column="h_info"></property>
		<property name="h_houseType" column="h_houseType"></property>
		<property name="h_url" column="h_url"></property>
		<property name="s_id" column="s_id"></property>
	</class>
</hibernate-mapping>
